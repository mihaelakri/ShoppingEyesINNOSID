<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ShoppingEyes" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <item name="colorPrimary">@android:color/transparent</item>
        <item name="colorPrimaryVariant">@android:color/transparent</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="titleTextColor">@color/white</item>
        <item name="iconTint">@color/white</item>
    </style>
    <style name="SecondTheme.ShoppingEyes" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/blackHC</item>
        <item name="colorPrimaryVariant">@color/blackHC</item>
        <item name="colorOnPrimary">@color/yellowHC</item>
        <item name="colorSecondary">@color/blackHC</item>
        <item name="colorSecondaryVariant">@color/blackHC</item>
        <item name="colorOnSecondary">@color/yellowHC</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="titleTextColor">@color/yellowHC</item>
        <item name="android:colorControlActivated">@color/yellowHC</item>
        <!-- Customize your theme here. -->
    </style>
</resources>